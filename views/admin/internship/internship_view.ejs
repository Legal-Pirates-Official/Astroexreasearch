<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <link rel="stylesheet" href="../../stylesheets/admin-nav.css" />
        <link
            href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
            rel="stylesheet"
            integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
            crossorigin="anonymous"
        />
        <title>Internship</title>
    </head>
	<link rel="shortcut icon" href="../../images/favicon.ico" type="image/x-icon">
    <%- include("../../partials/internship.ejs") %>

    <body>
        <%- include("../../partials/admin_nav.ejs") %>

        <div class="container">
            <div class="sub_container">
                <div class="row">
                    <div>
                        <h3>Title:</h3>
                        <p><%= internship.heading_internship %></p>
                    </div>
                    <div>
                        <h3>Sub Heading:</h3>
                        <p><%= internship.subheading_internship %></p>
                    </div>
                    <div>
                        <h3>Description:</h3>
                        <p><%= internship.description_internship %></p>
                    </div>
                    <div>
                        <h3>Form Link:</h3>
                        <p><%= internship.form_internship%></p>
                    </div>
                    <div>
                        <h3>Image:</h3>
                        <img src="<%= internship.image_internship%>" alt="">
                    </div>
                    <div>
                        <h3>Tag Line:</h3>
                        <p>
                            <ul>
                                <%internship.tag_internship.split(",").reverse().forEach((element, index) => { %>
                                <li><%= element.split("=")[1] %></li>
                                <% }); %>
                            </ul>
                        </p>
                    </div>
                </div>
            </div>
        </div>

        <script
            src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
            crossorigin="anonymous"
        ></script>
    </body>
</html>
