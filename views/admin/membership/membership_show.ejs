<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<link rel="stylesheet" href="../../stylesheets/membership-admin.css" />
		<link rel="stylesheet" href="../../stylesheets/admin-nav.css" />
		<link
			href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
			rel="stylesheet"
			integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
			crossorigin="anonymous"
		/>
		<link rel="stylesheet" href="../../stylesheets/admin-show.css" />
		<title>Membership</title>
	</head>
	<body>
		<%- include("../../partials/admin_nav.ejs") %>

		<section class="membership-section">
			<div class="button" style="width: 100%">
				<a href="/admin/membership/insert" class="btn btn-primary">Insert</a>
			</div>
			<div class="membership-main-div">
				<h1>Membership</h1>
				<p><%= para && para.paragraph %></p>
			</div>
			<div class="membership-table-top">
				<table>
					<tr>
						<th>Membership</th>
						<th>Student</th>
						<th>Professional</th>
						<!-- <th>Bronze Membership</th>
						<th>Institutional Membership</th> -->
					</tr>
					<% table1.forEach(tab => {%>
					<tr>
						<td><%=tab.input1_membership%></td>
						<td><%=tab.input2_membership%></td>
						<td><%=tab.input3_membership%></td>
						<!-- <td><%#=tab.input4_membership%></td>
						<td><%#=tab.input5_membership%></td> -->
					</tr>
					<% }); %>
				</table>
			</div>

			<div class="membership-table-next">
				<table>
					<tr>
						<th rowspan="2">Benefits</th>
						<th colspan="2">Student</th>
						<th colspan="2">Professional</th>
					</tr>
					<tr>
						<th>1 Year</th>
						<th>Life time</th>
						<th>1 Year</th>
						<th>Life time</th>
					</tr>
					<% table2.forEach(tab => {%>
					<tr>
						<td><%=tab.input1_membership%></td>
						<td><%=tab.input2_membership%></td>
						<td><%=tab.input3_membership%></td>
						<td><%=tab.input4_membership%></td>
						<td><%=tab.input5_membership%></td>
					</tr>
					<% }); %>
				</table>
			</div>
		</section>

		<script
			src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
			integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
			crossorigin="anonymous"
		></script>
	</body>
</html>