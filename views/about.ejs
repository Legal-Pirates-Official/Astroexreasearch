                        <div class="partners-div">
                            <% partners.forEach(element=> { %>
                                <img src="<%- element.image_partners %>" alt="Partners" />
                                <% }) %>
                        </div>
                </div>
                </marquee>
            </div>
        </section>
        <section class="section-partners">
            <h1>Collaborators</h1>
